	<SimulaDirective path="shoot">
		<SimulaConstant
			name="aerenchymaPhotosynthesisMitigation"
			unit="100%">
			0.5 <!--0 is no effect, 1 is full effect-->
		</SimulaConstant>
	</SimulaDirective>
